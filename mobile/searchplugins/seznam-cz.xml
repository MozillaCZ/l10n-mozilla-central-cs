<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Seznam</ShortName>
<Description>Integrované hledávání na Seznam.cz</Description>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">data:image/png;base64,</Image>
<Url type="application/x-suggestions+json" method="GET" template="http://suggest.fulltext.seznam.cz/fulltext_ff">
  <Param name="phrase" value="{searchTerms}"/>
</Url>
<Url type="text/html" method="GET" template="http://search.seznam.cz/">
  <Param name="q" value="{searchTerms}"/>
  <Param name="sourceid" value="SearchBox"/>
</Url>
<SearchForm>http://search.seznam.cz/</SearchForm>
</SearchPlugin>
